{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "removeComments": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES6",
    "noEmit": true,
    "allowJs": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "strict": true,
  },
}